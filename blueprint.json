{
  "schema": {
    "type": "app_blueprint",
    "version": {
      "major": 0,
      "minor": 1
    },
    "origin": "Engines Developer Studio 0.2"
  },
  "metadata": {
    "blueprint": {
      "version": {
        "major": 0,
        "minor": 0,
        "level": "alpha",
        "patch": 0
      }
    },
    "timestamp": "2019-02-18T11:15:28+11:00"
  },
  "software": {
    "base": {
      "name": "eds",
      "framework": "sinatra",
      "deployment_type": "web",
      "http_protocol": "https_only",
      "memory": {
        "required": 384,
        "recommended": 512
      }
    },
    "system_packages": [
      {
        "package": "git"
      },
      {
        "package": "libgit2-dev"
      },
      {
        "package": "cmake"
      }
    ],
    "installed_packages": [
      {
        "name": "BluePrint-Designer",
        "source_url": "https://github.com/EnginesOS/BluePrint-Designer.git",
        "destination": "./",
        "extraction_command": "git",
        "path_to_extracted": "BluePrint-Designer"
      }
    ],
    "service_configurations": [
      {
        "publisher_namespace": "EnginesSystem",
        "type_path": "filesystem/local/filesystem",
        "variables": {
          "service_name": "_Engines_Builder(engine_name)",
          "volume_src": " ",
          "engine_path": "git",
          "permissions": "rw",
          "user": "_Engines_Builder(data_uid)",
          "group": "_Engines_Builder(data_gid)"
        }
      },
      {
        "publisher_namespace": "EnginesSystem",
        "type_path": "database/sql/mysql",
        "variables": {
          "database_name": "_Engines_Builder(engine_name)",
          "database_host": "_Engines_System(mysql_host)",
          "db_username": "_Engines_Builder(engine_name)",
          "db_password": "_Engines_System(random(10))",
          "collation": "utf8_general_ci",
          "full_access": "0"
        }
      }
    ],
    "template_files": [
      {
        "path": "app/config/configuration.yml",
        "content": "production:\r\n  email_delivery:\r\n    delivery_method: :smtp\r\n    smtp_settings:\r\n      address: _Engines_System(smtp_host)\r\n      port: 25\r\n      domain: _Engines_System(default_domain)\r\n      enable_starttls_auto : false"
      }
    ],
    "file_write_permissions": [
      {
        "path": "./",
        "recursive": true
      }
    ],
    "framework_specific": {
      "rake_tasks": [
        {
          "action": "db:migrate",
          "always_run": true
        },
        {
          "action": "db:seed"
        },
        {
          "action": "assets:precompile",
          "always_run": true
        }
      ]
    }
  }
}
